{
  "tasks": {
    "dev": "deno run -A npm:vite",
    "built": "deno run -A npm:vite build",
    "hook": "deno run --allow-read --allow-run --allow-write https://deno.land/x/deno_hooks@0.1.2/mod.ts install"
  },
  "imports": {
    "@commitlint/cli": "npm:@commitlint/cli@^19.8.1",
