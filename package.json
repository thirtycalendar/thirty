{
  "name": "thirty",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "tunnel": "pnpm dlx localtunnel --port 5173",
    "tunnel:dev": "pnpm dlx localtunnel --port 5173 --subdomain thirty-calendar-dev || true",
    "deploy": "vercel --force",
    "prod": "vercel --force --prod",
    "build": "vite build",
    "prepare": "husky",
    "preview": "vite preview",
    "check": "svelte-kit sync && NODE_OPTIONS=--max-old-space-size=4096 svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier .",
    "lint": "eslint",
    "auth:generate": "pnpm dlx @better-auth/cli generate --config ./libs/auth/index.ts --output ./libs/db/schemas/auth-table.ts --y",
    "db:push": "cross-env NODE_ENV=development drizzle-kit push",
    "db:generate": "cross-env NODE_ENV=development drizzle-kit generate",
    "db:migrate": "cross-env NODE_ENV=development drizzle-kit migrate",
    "db:studio": "cross-env NODE_ENV=development drizzle-kit studio",
    "db:generate:prod": "cross-env NODE_ENV=production drizzle-kit generate",
    "db:migrate:prod": "cross-env NODE_ENV=production drizzle-kit migrate",
    "db:studio:prod": "cross-env NODE_ENV=production drizzle-kit studio"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.34.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@sveltejs/adapter-vercel": "^5.10.2",
    "@sveltejs/kit": "^2.37.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "@types/node": "^24.3.0",
    "commitlint": "^19.8.1",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.4",
    "vite-plugin-devtools-json": "^1.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "@googleapis/calendar": "^11.0.1",
    "@hono/zod-validator": "^0.7.2",
    "@hugeicons/core-free-icons": "^1.0.16",
    "@hugeicons/svelte": "^1.0.2",
    "@libsql/client": "^0.15.14",
    "@openrouter/ai-sdk-provider": "^1.1.2",
    "@polar-sh/better-auth": "^1.1.0",
    "@polar-sh/sdk": "^0.34.14",
    "@tailwindcss/vite": "^4.1.12",
    "@upstash/redis": "^1.35.3",
    "@upstash/vector": "^1.2.2",
    "@vvo/tzdb": "^6.183.0",
    "ai": "^5.0.29",
    "better-auth": "^1.3.7",
    "clsx": "^2.1.1",
    "daisyui": "^5.1.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.5",
    "googleapis": "^159.0.0",
    "hono": "^4.9.5",
    "openai": "^5.16.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "voyage-ai-provider": "^2.0.0",
    "zod": "^4.1.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "zod"
      ],
      "ignoreMissing": [
        "zod"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide"
    ]
  }
}
