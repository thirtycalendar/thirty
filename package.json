{
  "name": "thirty",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "tunnel": "instatunnel connect 5173 --subdomain thirty",
    "deploy": "vercel --force",
    "prod": "vercel --force --prod",
    "build": "vite build",
    "prepare": "husky",
    "preview": "vite preview",
    "check": "svelte-kit sync && NODE_OPTIONS=--max-old-space-size=4096 svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier .",
    "lint": "eslint",
    "auth:generate": "pnpm dlx @better-auth/cli generate --config ./src/lib/server/auth/index.ts --output ./src/lib/server/db/tables/auth.ts --y",
    "db:push": "dotenv -e .env -- drizzle-kit push",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "db:generate:prod": "dotenv -e .env.prod -- drizzle-kit generate",
    "db:migrate:prod": "dotenv -e .env.prod -- drizzle-kit migrate",
    "db:studio:prod": "dotenv -e .env.prod -- drizzle-kit studio"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@deno/svelte-adapter": "^0.1.0",
    "@eslint/compat": "^1.3.2",
    "@eslint/js": "^9.35.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@sveltejs/kit": "^2.41.0",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@types/node": "^24.5.0",
    "@types/pg": "^8.15.5",
    "commitlint": "^19.8.1",
    "dotenv": "^17.2.2",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-svelte": "^3.12.3",
    "eslint-plugin-unicorn": "^61.0.2",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.38.10",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5",
    "vite-plugin-devtools-json": "^1.0.0"
  },
  "dependencies": {
    "@ai-sdk/svelte": "^3.0.44",
    "@floating-ui/dom": "^1.7.4",
    "@googleapis/calendar": "^11.0.1",
    "@hono/zod-validator": "^0.7.2",
    "@hugeicons/core-free-icons": "^1.0.17",
    "@hugeicons/svelte": "^1.0.2",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@polar-sh/better-auth": "^1.1.4",
    "@polar-sh/sdk": "^0.34.17",
    "@tailwindcss/vite": "^4.1.13",
    "@upstash/redis": "^1.35.3",
    "@upstash/vector": "^1.2.2",
    "@vvo/tzdb": "^6.183.0",
    "ai": "^5.0.44",
    "better-auth": "^1.3.11",
    "clsx": "^2.1.1",
    "daisyui": "^5.1.12",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "^0.44.5",
    "googleapis": "^159.0.0",
    "hono": "^4.9.7",
    "openai": "^5.20.3",
    "pg": "^8.16.3",
    "svelte-markdown": "^0.4.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "voyage-ai-provider": "^2.0.0",
    "zod": "^4.1.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "zod"
      ],
      "ignoreMissing": [
        "zod"
      ]
    },
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@tailwindcss/oxide"
    ]
  }
}
